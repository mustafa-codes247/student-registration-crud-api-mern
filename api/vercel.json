// api/vercel.json (Use this configuration)
{
  "version": 2,
  "installCommand": "npm install",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    // FIX: Only proxy paths that start with /api/
    {
      "src": "/api/(.*)", 
      "dest": "index.js"
    }
  ]
}